<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Agenda</display-name>
  <welcome-file-list>
    <welcome-file>Agenda.do</welcome-file>
  </welcome-file-list>
  
  <servlet>
	<servlet-name>Ingresar</servlet-name>
	<servlet-class>org.josue.servlet.agenda.Ingresar</servlet-class>  
  </servlet>
  <servlet-mapping>
  	<servlet-name>Ingresar</servlet-name>
  	<url-pattern>/Ingresar.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
	<servlet-name>AgregarContacto</servlet-name>
	<servlet-class>org.josue.servlet.agenda.AgregarContacto</servlet-class>  
  </servlet>
  <servlet-mapping>
  	<servlet-name>AgregarContacto</servlet-name>
  	<url-pattern>/AgregarContacto.do</url-pattern>
  </servlet-mapping>
</web-app>